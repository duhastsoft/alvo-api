{
  "name": "alvo-stack-api",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js --config /src/database.ts",
    "migration:generate": "npm run typeorm -- migration:generate --name GeneratedDatabase",
    "migration:run": "npm run typeorm -- migration:run",
    "migration:create": "ts-node ./node_modules/typeorm/cli.js migration:create -n CreatedDatabase",
    "migrate:up": "ts-node ./node_modules/typeorm/cli.js migration:run",
    "migrate:down": "ts-node ./node_modules/typeorm/cli.js migration:revert",
    "start:dev": "nodemon",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --fix",
    "build": "tsc"
  },
  "keywords": [],
  "author": "",
  "devDependencies": {
    "@types/express": "^4.17.8",
    "@types/helmet": "0.0.48",
    "@types/morgan": "^1.9.1",
    "@types/node": "^14.11.2",
    "@typescript-eslint/eslint-plugin": "^4.4.0",
    "@typescript-eslint/parser": "^4.4.0",
    "eslint": "^7.10.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-plugin-import": "^2.22.1",
    "nodemon": "^2.0.4",
    "ts-node": "^9.0.0",
    "ts-node-dev": "^1.0.0-pre.63",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.0.3"
  },
  "dependencies": {
    "bcrypt": "^5.0.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-validator": "^6.6.1",
    "helmet": "^4.1.1",
    "install": "^0.13.0",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.10.0",
    "pg": "^8.3.3",
    "pg-hstore": "^2.3.3",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.2.27"
  }
}
